{
  "name": "dtd2mysql",
  "version": "3.0.0",
  "description": "Command line tool to put the GB rail DTD feed into a MySQL compatible database",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dtd2mysql": "dist/index.js"
  },
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register 'test/**/*.spec.ts'",
    "watch-test": "mocha -w --compilers ts:ts-node/register,tsx:ts-node/register 'test/**/*.spec.ts'",
    "run": "ts-node ./src/index.ts",
    "prepublish": "#tsc -p ./ --outDir dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-track/uk-rail-import.git"
  },
  "keywords": [
    "gb",
    "rail",
    "etl",
    "fares",
    "routeing",
    "timetable",
    "dtd"
  ],
  "author": "Linus Norton",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/open-track/uk-rail-import/issues"
  },
  "homepage": "https://github.com/open-track/uk-rail-import#readme",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "bluebird": "^3.5.0",
    "memoized-class-decorator": "^1.2.0",
    "moment": "^2.18.1",
    "mysql2": "^1.3.2",
    "ts-option": "^1.1.3"
  },
  "engines": {
    "node": "^6.9.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.32",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.5",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "ts-node": "^2.1.0",
    "typescript": "^2.3.0"
  }
}
