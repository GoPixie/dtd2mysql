{
  "name": "dtd2mysql",
  "version": "3.0.0",
  "description": "Command line tool to put the GB rail DTD feed into a MySQL compatible database",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dtd2mysql": "dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "run": "ts-node ./src/index.ts",
    "prepublish": "tsc -p ./ --outDir dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-track/uk-rail-import.git"
  },
  "keywords": [
    "gb",
    "rail",
    "etl",
    "fares",
    "routeing",
    "timetable",
    "dtd"
  ],
  "author": "Linus Norton",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/open-track/uk-rail-import/issues"
  },
  "homepage": "https://github.com/open-track/uk-rail-import#readme",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "bluebird": "^3.4.1",
    "moment": "^2.15.1",
    "promise-mysql": "^1.3.2",
  },
  "engines": {
    "node": "^6.9.0"
  },
  "devDependencies": {
    "@types/request": "0.0.30",
    "@types/bluebird": "^3.0.32",
    "@types/node": "^7.0.5",
    "typescript": "^2.2.0",
    "ts-node": "^2.1.0"
  }
